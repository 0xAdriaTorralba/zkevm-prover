{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "gcc - Build and debug active file",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/zkProver",
            "args": ["${workspaceFolder}/testvectors/input.json",
                    "-r","${workspaceFolder}/testvectors/rom.json",
                    "-p","${workspaceFolder}/testvectors/zkevm.pil.json",
                    "-o","${workspaceFolder}/testvectors/commit.bin",
                    "-c","${workspaceFolder}/testvectors/constants.bin",
                    "-t","${workspaceFolder}/testvectors/constantstree.bin",
                    "-x","${workspaceFolder}/testvectors/starkgen_bmscript.json",
                    "-s","${workspaceFolder}/testvectors/stark.json",
                    "-v","${workspaceFolder}/testvectors/verifier.dat",
                    "-w","${workspaceFolder}/testvectors/witness.wtns",
                    "-k","${workspaceFolder}/testvectors/starkverifier_0001.zkey",
                    "-f","${workspaceFolder}/testvectors/proof.json",
                    "-b","${workspaceFolder}/testvectors/public.json"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/testvectors",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "C/C++: gcc build",
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}